function animateValue(id, start, duration) {
  var obj = document.getElementById(id);
    var end=parseInt(obj.innerHTML);
    console.log(end); 
    if (start === end) return;
      var range = end - start;
      var current = start;
      var increment = end > start? 1 : -1;
      var stepTime = Math.abs(Math.floor(duration / range));
      var timer = setInterval(function() {
          current += increment;
          obj.innerHTML = current;
          if (current == end) {
              clearInterval(timer);
          }
      }, stepTime);
}

function checkvisible( elm ) { 	
  var vpH = $(window).height(), // Viewport Height 		
  st = $(window).scrollTop(), // Scroll Top 		
  y = $(elm).offset().top, 		
  h = $(elm).height(); 		 	
  return (y+h > st && y < (vpH + st)); 
}
var done1=0
function animate1(){
  if(done1==0 && checkvisible(document.getElementById("numberMentors"))){
    animateValue("numberMentors", 0, 1000);
    done1=1
  }
}
setInterval(animate1, 50);


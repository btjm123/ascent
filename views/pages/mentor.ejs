    <script>
function addFormatter (input, formatFn, callback) {
  let oldValue = input.value;
  
  const handleInput = event => {
    const result = formatFn(input.value, oldValue, event);
    if (typeof result === 'string') {
      input.value = result;
    }
    
    oldValue = input.value;

    if (callback !== undefined) {
      callback(input, result)
    }
  }

  handleInput();
  input.addEventListener("input", handleInput);
}

function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    return true;
}

function createRegexFormatter (regex, defaultValue) {
  return (newValue, oldValue) => regex.test(newValue)
    ? newValue
    : (newValue ? oldValue : defaultValue);
}

// suffix formatter
const input2 = document.getElementById('email');

function onFocus(){addFormatter(input2, createRegexFormatter(/\@vjc\.sg$/, '@vjc.sg'), input => {
  
  const pos = Math.max(0, input.value.length - 7)
  input.setSelectionRange(pos, pos);
  input2.style.textAlign="right";
})};

function getRndInteger(min, max) {
  return Math.floor(Math.random() * (max - min) ) + min;
}
var a=getRndInteger(1,16);
var b=getRndInteger(1,16);
var sum=a+b;
document.getElementById("captcha").innerHTML=a.toString() + " + " + b.toString() + " =";

function validateCaptcha() {
  console.log("here");
  var x = document.getElementById("captchaInput").value;
  if (x != sum.toString()) {
    console.log(x);
    document.getElementById("captchaInput").setCustomValidity('Please enter the correct number');
  }else{
    document.getElementById("captchaInput").setCustomValidity('');
  }
}

